{
    "name": "odan/plates-asset-cache",
    "type": "library",
    "description": "Caching and compression for Plates template assets (JavaScript and CSS).",
    "keywords": ["plates", "assets", "cache", "javascript", "css", "minify"],
    "homepage": "http://github.com/odan/plates-asset-cache",
    "license": "MIT",
    "authors": [
        {
            "name": "odan",
            "email": "support@molengo.com",
            "homepage": "http://www.molengo.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "league/plates": "3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Odan\\Jsin\\": "src/JsMin",
            "Odan\\CSSmin\\": "src/CssMin",
            "Odan\\Plates\\Extension\\": "src/Plates/Extension",
            "Odan\\Plates\\Test\\": "tests"
        }
    }
}
